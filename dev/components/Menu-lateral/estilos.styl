/*MENU LATERAL*/
	.MenuL
		display none
@media screen and (min-width:800px)

	.MenuL
		display inline-block
		right 0
		margin 0
		padding 0
		position fixed
		top 40%
		z-index 9999
		text-align center
		list-style none
		
		
		& li
			width 80px
			height 25px
			line-height 25px
			background #0090d0
			margin 8px 0
				
			&:nth-child(2) 
				a
					font-size 12.4px
					
			&:hover
				background #213f99
					
		& li a
			cursor pointer
			color $white
			display inline-block
			width 100%
			height 100%
			font-weight 400
			font-size 15.2px